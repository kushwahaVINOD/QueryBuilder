<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane id="AnchorPane" prefHeight="400.0" prefWidth="400.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.141" fx:controller="controller.LoginWimdowController">
   <children>
      <Label layoutX="145.0" layoutY="90.0" prefHeight="38.0" prefWidth="147.0" text="Oracle Database User Login" />
      <TextField fx:id="username" layoutX="126.0" layoutY="153.0" promptText="enter user name" />
      <PasswordField fx:id="password" layoutX="126.0" layoutY="200.0" promptText="enter password" />
      <Button layoutX="134.0" layoutY="254.0" mnemonicParsing="false" onAction="#loginAction" text="Login" />
      <Button layoutX="207.0" layoutY="254.0" mnemonicParsing="false" onAction="#onClear" text="Reset" />
   </children>
</AnchorPane>
